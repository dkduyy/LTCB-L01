#include <stdio.h>
#include <math.h>

int main()
{
	int x,y,z; 
	printf("nhap thang: "); scanf("%d",&x);
	printf("nhap nam "); scanf("%d",&y);
	
	if ((x==1),(x==3),(x==5),(x==7),(x==8),(x==10),(x==12))
		z=31;
	else  if ((x==4),(x==6),(x==9),(x==11))
		z=31;
		else if ((y % 4 == 0 && y % 100 != 0) || (y % 400 == 0))
			z=29;
			else z=28;
	
	printf("thang %d, nam %d co %d ngay",x,y,z);
	
	return 0;
	 
}
